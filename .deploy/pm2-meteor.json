{
  "appName": "myApp",
  "appLocation": {
    "local": "../"
  },
  "meteorSettingsLocation": "../imports/settings.json",
  "meteorBuildFlags": "--architecture os.linux.x86_64",
  "env": {
    "ROOT_URL": "MY_DOMAIN_OR_IP",
    "PORT": 3005,
    "MONGO_URL": "mongodb://localhost:27017/myApp"
  },
  "server": {
    "host": "MY_DOMAIN_OR_IP",
    "username": "myUsername",
    "pem": "../private/my_SSH_key_mup_rsa",
    "deploymentDir": "/home/myApp",
    "loadProfile": "",
    "exec_mode": "cluster_mode",
    "instances": 1
  }
}